{
  "env": {
    "NEXT_PUBLIC_API_BASE_URL": "https://api-gateway-k9zw.onrender.com"
  },
  "functions": {
    "app/**": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Accept, Authorization, Content-Type, X-Requested-With"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://api-gateway-k9zw.onrender.com/api/$1"
    }
  ]
}
